@using System.Web.Optimization
@model IBuyStuff.Application.ViewModels.ViewModelBase

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link rel="shortcut icon" href="~/favicon.ico" />
    <title>I•Buy•Stuff </title>
    @Styles.Render("~/Bundles/Css")
    @Scripts.Render("~/Bundles/Core")
    @Scripts.Render("~/Bundles/Bootstrap")
    @RenderSection("CustomScripts", required: false) 
</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        @Html.Partial("desktop_p_header")
    </nav>

    <div class="container">
        @RenderBody()
        <div class="clearfix"></div>
    </div>
    
    @Html.Partial("desktop_p_technology")
    <div class="clearfix"></div>

    @Html.Partial("desktop_p_social")
    <div class="clearfix"></div>

    <footer>
        @Html.Partial("desktop_p_footer", Model)
        <div class="clearfix"></div>
    </footer>
</body>
</html>